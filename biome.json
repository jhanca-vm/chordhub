{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"organizeImports": {
		"enabled": true
	},
	"javascript": {
		"formatter": {
			"arrowParentheses": "asNeeded",
			"quoteStyle": "single",
			"semicolons": "asNeeded",
			"trailingComma": "none"
		}
	}
}
